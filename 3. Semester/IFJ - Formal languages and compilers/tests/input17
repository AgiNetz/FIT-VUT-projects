
scope 

Dim between as Integer = 65
Dim in as String
Input in

Dim l as Integer
l = Length(in)
print l; !"\n";

for i as Integer = 0 to l-1 step 1

	print Asc(in, i+1);
	i = i + 1
	print Chr(between);
	between = between + 1
next i

print !"\n\n";

print substr(in, l/2, 10000000); !"\n";

end scope
